{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
          {
            "source": "**",
            "headers": [
              {
                "key": "Content-Security-Policy",
                "value": "frame-ancestors 'self' https://github.com"
              }
            ]
          }
        ]
  },
  "database": {
    "rules": "database.rules.json"
  }
}
